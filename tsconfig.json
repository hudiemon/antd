{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@hudiemon/antd": [
        "src"
      ],
      "@hudiemon/antd/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*"
  ]
}
